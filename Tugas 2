import java.util.Scanner;

public class Pertemuan3 {
    public static void main(String[] args) {
        int A, B, C;
        
        Scanner keyboard = new Scanner(System.in);
        
        System.out.print("Input data penjualan 1 : ");
        A = keyboard.nextInt();
        
        if (A > 1500) {
            System.out.println("BERHASIL");
        } else if (A == 1500) {
            System.out.println("PAS");
        } else {
            System.out.println("KURANG PAS");
        }
        System.out.println("========================");
        
        System.out.print("Input data penjualan 2 : ");
        B = keyboard.nextInt();
        
        if (B > 1500) {
            System.out.println("BERHASIL");
        } else if (B == 1500) {
            System.out.println("PAS");
        } else {
            System.out.println("KURANG PAS");
        }
        System.out.println("========================");
        
        System.out.print("Input data penjualan 3 : ");
        C = keyboard.nextInt();
        
        if (C > 1500) {
            System.out.println("BERHASIL");
        } else if (C == 1500) {
            System.out.println("PAS");
        } else {
            System.out.println("KURANG PAS");
        }
        System.out.println("========================");
    }
}

