class HelloWorld {
    public static void main(String[] args) {
        
    int x = 3;
    if ( x == 1){
        System.out.println("Nilai yang di isi adalah A = " + x );
    }
    if ( x == 2){
        System.out.println("Nilai yang di isi adalah A = " + x );
    }
    if ( x == 3){
        System.out.println("Nilai yang di isi adalah A = " + x );
    } else {
        System.out.println("Nilai yang di isi adalah tidak sama semua");
    }
    }
}

